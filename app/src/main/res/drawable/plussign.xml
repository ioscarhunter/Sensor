<?xml version="1.0" encoding="utf-8"?>

<layer-list xmlns:android="http://schemas.android.com/apk/res/android">
    <item>
        <shape
            android:shape="rectangle">
            <solid android:color="@color/pink_heart"/>
            <size
                android:width="2dp"
                android:height="30dp"/>
        </shape>
    </item>
    <item
        android:bottom="1dp"
        android:left="1dp"
        android:right="1dp"
        android:top="1dp">
        <shape android:shape="rectangle">
            <solid android:color="@color/yeelow_egg"/>
            <size
                android:width="30dp"
                android:height="2dp"/>
        </shape>
    </item>
</layer-list>


